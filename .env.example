# Flask Configuration
LOG_LEVEL=INFO

# QuickBase Configuration
QB_REALMID=your_quickbase_realm_id
QB_TABLEID=your_quickbase_table_id
QB_USER_TOKEN=your_quickbase_user_token
PAGE_SIZE=1000

# GCP Configuration
PROJECT_ID=your-gcp-project-id
BQ_LOCATION=US
GCP_CREDENTIALS_PATH=/app/credentials/service-account.json

# GCS Configuration
BUCKET=your-gcs-bucket-name
GCS_PREFIX=quickbase_exports/
COMPRESS_JSONL=true

# BigQuery Tables
BQ_STAGING_DATASET=staging
BQ_STAGING_TABLE=quickbase_staging
BQ_WORK_DATASET=warehouse
BQ_WORK_TABLE=quickbase_work
BQ_CORE_DATASET=core
BQ_CORE_TABLE=quickbase_core


